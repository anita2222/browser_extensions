{
    "manifest_version": 2,
    "key": "",
    "name": "辅助诊疗",
    "description": "辅助诊疗扩展程序，优化Chrome浏览器下辅助诊疗系统的访问",
    "version": "1.0",

    "browser_action": {
        "default_icon": "DiagnoseAssistantIcon.png"
        //"default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab"
        //"http://*/*",
        //"https://*/*"
        //,"*://localhost:8080/*"
    ],
    "background": {
        "scripts": [ "background.js" ]
    },
    "externally_connectable": {
        "matches": ["*://172.26.111.12/*"]
    }
}
